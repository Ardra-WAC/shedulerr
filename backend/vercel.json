{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "backend/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/api/index.js"
        }
    ],
    "functions": {
        "backend/api/index.js": {
            "runtime": "nodejs18.x"
        }
    }
}